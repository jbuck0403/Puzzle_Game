		_Albedo("Albedo", 2D) = "white" {}
		_Metallic("Metallic", 2D) = "white" {}
		_Metallic_Intensity("Metallic_Intensity", Range( 0 , 3)) = 0
		_Emission("Emission", 2D) = "white" {}
		_Emission_Intensity("Emission_Intensity", Range( 0 , 3)) = 0
		_AmbientOcclusion("Ambient Occlusion", 2D) = "white" {}
		_Opacity("Opacity", 2D) = "white" {}
		_Opacity_Intensity("Opacity_Intensity", Range( 0 , 1)) = 0
		_Roughness("Roughness", 2D) = "white" {}
		_Roughness_Intensity("Roughness_Intensity", Range( 0 , 10)) = 0
		_Normal("Normal", 2D) = "bump" {}
